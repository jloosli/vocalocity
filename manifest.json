{
	"manifest_version":2,

	"name":"Vocalocity Calling",
	"description":"Easy dial for Vocalocity",
	"version":"0.0.0.4",

	"permissions":[
		"contextMenus",
		"https://my.vocalocity.com/",
        "https://dashboard.vocalocity.com/",
		"storage"
	],
	"content_security_policy" : "script-src 'self' https://my.vocalocity.com/ https://dashboard.vocalocity.com/; object-src 'self'",
	"browser_action":{
		"default_icon":"icons/icon-16.png",
		"default_popup":"popup.html"
	},
	"icons":{
		"16":"icons/icon-16.png",
		"48":"icons/icon-48.png",
		"128":"icons/icon-48.png"
	}
}